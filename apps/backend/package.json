    "@apollo/server-plugin-landing-page-graphql-playground": "^4.0.1",
    "@as-integrations/express4": "^1.1.2",
    "body-parser": "^1.20.0",
    "express": "^4.18.2",
    "graphql": "^16.12.0",
    "pg": "^8.17.1",
    "reflect-metadata": "^0.2.2",
